<h1>Welcome to the Team Site of <%= teamSite.escape("name") %></h1>
<h2>Users:</h2>
<ul>
  <% users.each(function (user) { %>
    <li><%= user.get("username") %></li>
  <% }) %>
</ul>

<h2>Channels:</h2>
<ul>
  <% channels.each(function (channel) { %>
    <li><%= channel.get("title") %></li>
  <% }) %>
</ul>
